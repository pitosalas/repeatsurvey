<div class="page-header">
   <h1>Program '<%= @program.name %>'</h1>
</div>

<div class="tabbable">
   <%= render "program_tab_headers" %>

   <div class="tab-content">
      <div id="report-pane" class="tab-pane active">
         <h3>Reports</h3>
      </div>
      <div id="summary-pane" class="tab-pane">
         <h3>Summary Statistics</h3>
      </div>
      <div id="setting-pane" class="tab-pane">
         <h3>Project Settings</h3>
      </div>
      <div id="participant-pane" class="tab-pane">
         <h3>Participants</h3>
         <table class="table table-condensed table-bordered">
            <thead>
               <tr>
                  <th>Participant Name</th>
               </tr>
            </thead>
            <tbody>
               <%= render @respondents || "There are no Participant yet" %>
            </tbody>
         </table>
      </div>
      <div id="question-pane" class="tab-pane">
         <%= render 'questions_pane' %>
      </div>
      <div id="round-pane" class="tab-pane">
         <%= render 'rounds_pane' %>
      </div>
   </div>
</div>
<%= debug_enable false, @program, @respondents, @questions %>
